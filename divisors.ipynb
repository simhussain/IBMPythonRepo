   "source": [
    "# program to print all divisors of user provided number\n",
    "\n",
    "num=int(input(\"Enter a number\"))\n",
    "divs=[]\n",
    "for i in range(2,num):\n",
    "    if (num%i==0):\n",
    "        divs.append(i)\n",
    "\n",
    "print(\"This is the list of all divisors of number\",num,\":\",divs)"
